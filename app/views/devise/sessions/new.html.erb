<nav class="first">
  <ul class="second list-unstyled">
    <li class="arrow-back">
      <%= link_to '<', new_registration_path(resource_name), class: "arrow-back" %>
    </li>
    <li>
      <h2 class="logs-in">LOGIN</h2>
    </li>
    <li>
      <i class="next-non">Log in</i>
    </li>
  </ul>
</nav>
<section>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <ul class="list-unstyled">
  <li class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email", class: "email1" %>
  </li>
  <hr />
  <li class="field">
    <%= f.password_field :password, autocomplete: "new-password", placeholder: "Password(6 characters)", class: "password1" %>
  </li>

  <% if devise_mapping.rememberable? %>
    <li class="field">
      <%= f.check_box :remember_me, class: 'no-ne' %>
      <%= f.label :remember_me, class: 'no-ne' %>
    </li>
  <% end %>

  <li class="field">
    <%= f.submit "Log in", class: 'btn-primary' %>
  </li>
  </ul>
<% end %>
</section>
